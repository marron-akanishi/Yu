{
    "manifest_version": 3,
    "name": "Yu",
    "version": "1.0.0",
    "description": "TOKIMEKI Backup",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://tokimeki.blue/"],
        "js": ["main.js"],
        "run_at": "document_start"
      }
    ],
    "permissions": [
      "storage"
    ],
    "host_permissions": ["https://tokimeki.blue/"]
  }